{
  "name": "shopwale",
  "version": "2.0.0",
  "description": "eCommerce website with MERN stack ",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server \"  \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install && npm install --prefix fronted && npm run build --prefix fronted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhruvpatel007/shopwale.git"
  },
  "author": "Dhruv",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dhruvpatel007/shopwale/issues"
  },
  "homepage": "https://github.com/Dhruvpatel007/shopwale#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
